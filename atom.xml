---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Frederik Dietz</title>
 <link href="http://fdietz.de/atom.xml" rel="self"/>
 <link href="http://fdietz.de/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://fdietz.de/</id>
 <author>
   <name>Frederik Dietz</name>
   <email>fdietz@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://fdietz.de{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://fdietz.de{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>